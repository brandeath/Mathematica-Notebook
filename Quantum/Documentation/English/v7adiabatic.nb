Cell[BoxData[
 RowBox[{"ReplaceAll", "[", 
  RowBox[{"myexpr", ",", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      SubscriptBox["q", "0"], "\[Rule]", "True"}], ",", 
     RowBox[{
      SubscriptBox["q", "1"], "\[Rule]", "True"}], ",", 
     RowBox[{
      SubscriptBox["q", "2"], "\[Rule]", "False"}], ",", 
     RowBox[{
      SubscriptBox["q", "3"], "\[Rule]", "True"}]}], "}"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.483470939365035*^9, 3.48347098619346*^9}, {
  3.4844915734020524`*^9, 3.4844915958547463`*^9}}],

