Cell[BoxData[{
 RowBox[{
  RowBox[{"inihamilt", "=", 
   RowBox[{
    UnderoverscriptBox["\[Sum]", 
     RowBox[{"j", "=", "0"}], "3"], 
    RowBox[{
     RowBox[{"1", "/", "2"}], 
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["\[Sigma]", 
        RowBox[{"\[ScriptZero]", ",", 
         OverscriptBox["j", "^"]}]], "-", 
       SubscriptBox["\[Sigma]", 
        RowBox[{"\[ScriptCapitalX]", ",", 
         OverscriptBox["j", "^"]}]]}], ")"}]}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"hamilt", "[", "t_", "]"}], ":=", " ", 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{"1", "-", "t"}], ")"}], "*", "inihamilt"}], " ", "+", " ", 
    RowBox[{"t", "*", "myhamilt"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
