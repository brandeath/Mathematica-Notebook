Cell["\<\
Given that the system started at the lowest energy state of the initial \
Hamiltonian and that the evolution is adiabatic, the system will end in \
lowest energy state of the final Hamiltonian:\
\>", "Text",
 CellChangeTimes->{{3.4834707250980387`*^9, 3.4834708126454744`*^9}, 
   3.4862990178560996`*^9}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"QuantumEigensystemForm", "[", 
  RowBox[{"myhamilt", ",", 
   RowBox[{"-", "1"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.483470692316579*^9, 3.483470703207274*^9}}],

Cell[BoxData[
 TagBox[GridBox[{
    {"\<\"Eigenvalue\"\>", "\<\"Eigenvector\"\>"},
    {"0", 
     TagBox[
      RowBox[{"\[VerticalSeparator]", 
       TagBox[
        RowBox[{
         SubscriptBox["1", 
          OverscriptBox["0", "^"]], ",", 
         SubscriptBox["1", 
          OverscriptBox["1", "^"]], ",", 
         SubscriptBox["0", 
          OverscriptBox["2", "^"]], ",", 
         SubscriptBox["1", 
          OverscriptBox["3", "^"]]}],
        Quantum`Notation`zz080KetArgs,
        BaseStyle->{ShowSyntaxStyles -> True},
        Editable->True,
        Selectable->True], "\[RightAngleBracket]"}],
      Quantum`Notation`zz080Ket,
      BaseStyle->{ShowSyntaxStyles -> False},
      Editable->False,
