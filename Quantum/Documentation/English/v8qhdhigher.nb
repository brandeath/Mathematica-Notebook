Cell[CellGroupData[{

Cell["Dependence of Decay Time on \[HBar]", "Section",
 CellChangeTimes->{{3.51118939934375*^9, 3.51118941846875*^9}}],

Cell[TextData[{
 "Next commands have been written as they would be used in a different ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " session, using the command Get to load the hierarchy that was saved above \
(instead of calculating it again). Notice that the commutation relation and \
the Hamiltonian are defined again, and its important to use those that \
correspond to the hierarchy that is loaded. Next commands calculate and graph \
the dependence of decay time on \[HBar], the result is stored in the variable \
data2, which will be used later to reproduce a graph of the paper by Pahl and \
Prezhdo, see the graph below:"
}], "Text",
 CellChangeTimes->{{3.515256702515625*^9, 3.515256831609375*^9}, {
  3.51525699525*^9, 3.51525711784375*^9}}],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"Needs", "[", "\"\<Quantum`QHD`\>\"", "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"SetQuantumObject", "[", 
   RowBox[{"q", ",", "p"}], "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   TagBox[
    SubscriptBox[
     RowBox[{"\[LeftDoubleBracket]", 
      TagBox[
       RowBox[{"q", ",", "p"}],
       Quantum`Notation`zz080KetArgs,
       Editable->True,
       Selectable->True], "\[RightDoubleBracket]"}], "-"],
    Quantum`Notation`zz050Commutator,
    Editable->False,
    Selectable->False], "=", 
   RowBox[{"\[ImaginaryI]", "*", "\[HBar]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"mh", "=", 
   RowBox[{
    FractionBox[
     SuperscriptBox["p", "2"], 
     RowBox[{"2", "m"}]], "+", 
    RowBox[{
     FractionBox[
      SubscriptBox["a", "2"], "2"], 
     SuperscriptBox["q", "2"]}], "+", 
    RowBox[{
     FractionBox[
      SubscriptBox["a", "3"], "3"], 
     SuperscriptBox["q", "3"]}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"hhier2", "=", 
   RowBox[{"Get", "[", "\"\<qhd2.m\>\"", "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"iniguess", "=", "15"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"data2", "=", 
   RowBox[{"Table", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"hnumhier2", "=", 
       RowBox[{"hhier2", "/.", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"m", "\[Rule]", "1"}], ",", 
          RowBox[{
           SubscriptBox["a", "2"], "\[Rule]", "1"}], ",", 
          RowBox[{
           SubscriptBox["a", "3"], "\[Rule]", 
           FractionBox["1", "2"]}], ",", 
          RowBox[{"\[HBar]", "\[Rule]", "h"}]}], "}"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"hinicond2", "=", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"\[LeftAngleBracket]", "p", "\[RightAngleBracket]"}], "[",
             "0", "]"}], "\[Equal]", "po"}], ",", 
          RowBox[{
           RowBox[{
            RowBox[{"\[LeftAngleBracket]", "q", "\[RightAngleBracket]"}], "[",
             "0", "]"}], "\[Equal]", "qo"}], ",", 
          RowBox[{
           RowBox[{
            RowBox[{"\[LeftAngleBracket]", 
             SuperscriptBox["q", "2"], "\[RightAngleBracket]"}], "[", "0", 
            "]"}], "\[Equal]", 
           RowBox[{
            SuperscriptBox["qo", "2"], "+", 
            FractionBox["\[HBar]", 
             RowBox[{"2", "m", "*", "\[Omega]"}]]}]}], ",", 
          RowBox[{
           RowBox[{
            RowBox[{"\[LeftAngleBracket]", 
             SubscriptBox[
              RowBox[{"(", 
               RowBox[{"p", "\[CenterDot]", "q"}], ")"}], "\[ScriptS]"], 
             "\[RightAngleBracket]"}], "[", "0", "]"}], "\[Equal]", 
           RowBox[{"po", "*", "qo"}]}], ",", 
          RowBox[{
           RowBox[{
            RowBox[{"\[LeftAngleBracket]", 
             SuperscriptBox["p", "2"], "\[RightAngleBracket]"}], "[", "0", 
            "]"}], "\[Equal]", 
           RowBox[{
            SuperscriptBox["po", "2"], "+", 
            FractionBox[
             RowBox[{"\[HBar]", "*", "m", "*", "\[Omega]"}], "2"]}]}]}], 
         "}"}], "/.", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"qo", "\[Rule]", "0"}], ",", 
          RowBox[{"po", "\[Rule]", "0"}], ",", 
          RowBox[{"\[Omega]", "\[Rule]", "1"}], ",", 
          RowBox[{"m", "\[Rule]", "1"}], ",", 
          RowBox[{"\[HBar]", "\[Rule]", "h"}]}], "}"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"hsol2", "=", 
       RowBox[{"QHDNDSolve", "[", 
        RowBox[{"hnumhier2", ",", "hinicond2", ",", "0", ",", 
         RowBox[{"iniguess", "+", "0.5"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"hqf2", "=", 
       RowBox[{"QHDFunction", "[", 
        RowBox[{"q", ",", "hsol2"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"htime", "=", 
       RowBox[{"(", 
        RowBox[{"ht", "/.", 
         RowBox[{"FindRoot", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"hqf2", "[", "ht", "]"}], "\[Equal]", 
            RowBox[{"-", "2"}]}], ",", 
           RowBox[{"{", 
            RowBox[{"ht", ",", "iniguess"}], "}"}], ",", 
           RowBox[{"AccuracyGoal", "\[Rule]", "3"}], ",", 
           RowBox[{"PrecisionGoal", "\[Rule]", "3"}], ",", 
           RowBox[{"MaxIterations", "\[Rule]", "1000"}]}], "]"}]}], ")"}]}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"iniguess", "=", "htime"}], ";", "\[IndentingNewLine]", 
      RowBox[{"{", 
       RowBox[{"h", ",", "htime"}], "}"}]}], ",", "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"h", ",", "0.85", ",", "2", ",", "0.025"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"ListLinePlot", "[", 
  RowBox[{"data2", ",", 
   RowBox[{"AxesLabel", "->", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"Style", "[", 
       RowBox[{"\[HBar]", ",", "Large"}], "]"}], ",", 
      "\"\<crossing time \[Tau]\>\""}], "}"}]}]}], "]"}]}], "Input",
 CellChangeTimes->{{3.511190828828125*^9, 3.511190935046875*^9}, {
   3.511190995046875*^9, 3.511191096765625*^9}, {3.5111914300625*^9, 
