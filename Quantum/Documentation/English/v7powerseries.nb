Cell[TextData[{
 "Here we use the standard ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " commands Exp, Series and Normal, in order to define an operator that is a \
(fifth power) approximation of the exponential of operator qc:"
}], "Text",
 CellChangeTimes->{{3.420574844676447*^9, 3.4205749135151443`*^9}, {
  3.420635112859375*^9, 3.4206351815625*^9}, {3.42071905178125*^9, 
  3.420719067359375*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"operatorFromSeries", "=", 
  RowBox[{"Normal", "[", 
   RowBox[{"Series", "[", 
    RowBox[{
     RowBox[{"Exp", "[", "qc", "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"qc", ",", "0", ",", "5"}], "}"}]}], "]"}], "]"}]}]], "Input",
 CellChangeTimes->{{3.4205747996851625`*^9, 3.4205748408789887`*^9}, {
  3.420719095875*^9, 3.42071910403125*^9}, {3.42071922821875*^9, 
  3.420719231015625*^9}}],

Cell[BoxData[
 RowBox[{"1", "+", "qc", "+", 
  FractionBox[
   SuperscriptBox["qc", "2"], "2"], "+", 
  FractionBox[
   SuperscriptBox["qc", "3"], "6"], "+", 
  FractionBox[
   SuperscriptBox["qc", "4"], "24"], "+", 
  FractionBox[
   SuperscriptBox["qc", "5"], "120"]}]], "Output",
 CellChangeTimes->{3.486734362121866*^9}]
}, Open  ]],

